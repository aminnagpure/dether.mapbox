{
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "react/forbid-prop-types": 0,
      "jsx-a11y/media-has-caption": 0,
      "no-console": 0,
      "no-underscore-dangle": 0,
      "no-global-assign": 0,
      "import/prefer-default-export": 0,
      "import/no-named-as-default": 0
    }
}
